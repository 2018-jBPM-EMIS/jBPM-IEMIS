<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.nkl.admin.domain.Plan" table="plan">
		<id name="plan_id" type="java.lang.Integer">
			<column name="plan_id" />
			<generator class="native" />
		</id>
		<property name="plan_year" type="java.lang.Integer">
			<column name="plan_year"  />
		</property>
		<property name="plan_year_half" type="java.lang.Integer">
			<column name="plan_year_half"  />
		</property>
		<property name="clazz_id" type="java.lang.Integer">
			<column name="clazz_id"  />
		</property>
		<property name="course_id" type="java.lang.Integer">
			<column name="course_id"  />
		</property>
		<property name="user_id" type="java.lang.Integer">
			<column name="user_id"  />
		</property>
		<property name="note" type="java.lang.String">
			<column name="note"  length="300" />
		</property>
	</class>
</hibernate-mapping>
