<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.nkl.admin.domain.Cplan" table="cplan">
		<id name="cplan_id" type="java.lang.Integer">
			<column name="cplan_id" />
			<generator class="native" />
		</id>
		<property name="cplan_year" type="java.lang.Integer">
			<column name="cplan_year"  />
		</property>
		<property name="cplan_year_half" type="java.lang.Integer">
			<column name="cplan_year_half"  />
		</property>
		<property name="clazz_id" type="java.lang.Integer">
			<column name="clazz_id"  />
		</property>
		<property name="cplan_week" type="java.lang.Integer">
			<column name="cplan_week"  />
		</property>
		<property name="cplan_lesson" type="java.lang.Integer">
			<column name="cplan_lesson"  />
		</property>
		<property name="course_id" type="java.lang.Integer">
			<column name="course_id"  />
		</property>
		<property name="note" type="java.lang.String">
			<column name="note"  length="300" />
		</property>
	</class>
</hibernate-mapping>
